	"scripts": {
		"test_dev": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest --verbose",
		"test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest --verbose",
		"sum": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest summarize --verbose",
		"maths": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest maths --verbose",
		"build": "npm run clean &&  npm run chrome",
		"format": "prettier --write .",
		"check": "prettier --check .",
		"lint": "eslint --ignore-path .gitignore --fix --ext .js,.vue .",
		"a11y": "eslint app/**/*.vue --ignore-path .gitignore --fix  && eslint app/**/*.js --ignore-path .gitignore --fix ",
		"css": "npx tailwindcss -i ./app/scripts/extension/index.css -o ./app/scripts/extension/outline.css",
		"chrome": "browser=chrome webpack   --stats detailed  ",
		"edge": "browser=edge webpack --stats detailed  ",
		"firefox": "browser=firefox webpack  --stats detailed ",
		"ios": "npm run clean &&  browser=ios webpack  --stats detailed  ",
		"clean": "rm -rf ./dist && rm -rf ./build",
		"links": "node links.js",
		"images": "node images.js",
		"button": "node buttons.js",
		"update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save",
	},
